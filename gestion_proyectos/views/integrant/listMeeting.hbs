<section class="content-header" style="position: relative;">
  <h1 class="flex-horizontal-start">
    <b style="margin-right:2%;">Lista de Reuniones</b>
  </h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-tasks"></i>Actividades</a></li>
    <li class="active">Listar</li>
  </ol>
</section>
<hr>
<section class="content">
  <div class="row">
    <div class="col-md-12">
      <label>{{error}}</label>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <table class="table table-striped table-condensed table-hover table-bordered">
        <thead>
          <tr>
            <th>Proyecto</th>
            <th>Temática</th>
            <th>Ubicación</th>
            <th>Fecha</th>
          </tr>
        </thead>
        <tbody>
          {{#each meetings}}
            <tr>
              <td>{{name_project}}</td>
              <td>{{thematic}}</td>
              <td>{{ubication}}</td>
              <td>{{start}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</section>
